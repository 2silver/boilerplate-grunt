{
  "name": "fhstp.cdnexample",
  "version": "1.0.0",
  "description": "A simple sample project to demonstrate fhstp's gitlab ci workflow",
  "scripts": {
    "--------------Test----------": "",
    "test": "grunt test",
    "--------------Basic----------": "",
    "clean:css": "rm -rf ./dist/styles",
    "clean:js": "rm -rf ./dist/js",
    "clean:img": "rm -rf ./dist/images",
    "clean": "rm -rf ./dist ./public ./.tmp ./.stylelintcache",
    "--------------Extensions----------": "",
    "accessibility": "a11y --delay=3 'dist/**/*.html'",
    "accessibility:afterbuild": "a11y --delay=3 'dist/**/*.html'",
    "accessibility:staging": "a11y --delay=3 staging.fhstp.ac.at",
    "accessibility:www": "a11y --delay=3 www.fhstp.ac.at",
    "dev": "yarn clean && NODE_ENV=${NODE_ENV:=development} && echo $NODE_ENV && grunt dev",
    "--------------PROD----------": "",
    "prebuild": "grunt test",
    "build": "yarn clean && NODE_ENV=${NODE_ENV:=production} && echo $NODE_ENV Enviroment && grunt"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": "500"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.fhstp.ac.at/cdn/fhstp.cdnexample"
  },
  "bugs": "https://gitlab.fhstp.ac.at/cdn/fhstp.cdnexample/issues",
  "author": "Michael Graf <michael.graf@fhstp.ac.at>",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "a11y": "^0.5.1",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "clean-css-cli": "^4.3.0",
    "diff": "^4.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "foreach-cli": "^1.8.1",
    "grunt": "^1.0.4",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^22.0.0",
    "grunt-node-sass": "^1.0.1",
    "grunt-nunjucks-2-html": "^3.0.0",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.0.2",
    "grunt-stylelint": "^0.11.1",
    "grunt-svgmin": "^6.0.0",
    "http-server": "^0.11.1",
    "imagemin-cli": "^5.0.0",
    "load-grunt-config": "^3.0.0",
    "load-grunt-tasks": "^5.1.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "opener": "^1.5.1",
    "postcss": "^7.0.17",
    "postcss-banner": "^3.0.1",
    "postcss-cli": "^6.1.3",
    "postcss-normalize-charset": "^4.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass": "^1.22.9",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-order": "^3.0.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.3",
    "svgo": "^1.3.0",
    "time-grunt": "^2.0.0",
    "watch": "^1.0.2"
  }
}
